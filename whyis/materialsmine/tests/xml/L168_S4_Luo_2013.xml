            <ChemicalUsed>
              <description>AgNO3</description>
            </ChemicalUsed>
            <ChemicalUsed>
              <description>ethylene glycol</description>
            </ChemicalUsed>
          </Mixing>
        </ChooseParameter>
        <ChooseParameter>
          <Additive>
            <Description>Followed by the addition of 5 g of BaTiO3 under magnetic stirring</Description>
