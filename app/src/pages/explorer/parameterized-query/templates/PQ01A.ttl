materialsmine_templates:PQ01A a whyis:SparqlTemplate;
  rdfs:label "Polymer nanocomposites: Two properties";
  spin:labelTemplate "Show me <b>polymer nanocomposite samples</b> containing {?Filler} filler surrounded by {?Matrix} matrix, along with the {?AmountType} fraction of filler and reported values of {?AttributeType1} and {?AttributeType2}.";
  sp:text """
PREFIX sio: <http://semanticscience.org/resource/>
PREFIX mm: <http://materialsmine.org/ns/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
SELECT DISTINCT * WHERE {
  ?Sample a mm:PolymerNanocomposite ;
          sio:hasComponentPart [ sio:hasRole [ a mm:Filler ] ;
                                 a [ rdfs:label ?Filler ] ;
                                 sio:hasAttribute [ a ?AmountType ;
                                                    sio:hasValue ?FillerAmount] ] ,
                               [ sio:hasRole [ a mm:Matrix ] ;
                                 a [ rdfs:label ?Matrix ] ] ;
          sio:hasAttribute [ a ?AttributeType1 ;
                             sio:hasValue ?AttributeValue1 ;
                             sio:hasUnit ?UnitOfMeasure1 ] ;
          sio:hasAttribute [ a ?AttributeType2 ;
                             sio:hasValue ?AttributeValue2 ;
                             sio:hasUnit ?UnitOfMeasure2 ] .
}
""";
